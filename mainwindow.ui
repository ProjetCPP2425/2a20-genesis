    padding: 6px;
    font-size: 14px;
    background-color: #ffffff;
}

QLineEdit:focus,
QTextEdit:focus,
QComboBox:focus,
QDateEdit:focus,
QTimeEdit:focus {
    border: 2px solid #213555;
}

/* ðŸŒŸ Buttons */
