        x1: 0, y1: 0,
        x2: 1, y2: 1,
        stop: 0 #f5efe7,
        stop: 1 #e6e0d7
    );
}

/* ðŸŒŸ Group Box - Forms &amp; Sections */
QGroupBox {
    background-color: #f5efe7;
    border: 2px solid #3e5879;
    border-radius: 10px;
    margin-top: 20px;
    padding: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    background-color: #3e5879;
    color: white;
    padding: 6px 12px;
    border-radius: 5px;
    font-size: 14px;
    font-weight: bold;
}

