    background-color: #3e5879;
    color: #ffffff;
    border-radius: 12px;
    padding: 10px 20px;
    font-size: 14px;
    font-weight: bold;
    border: 2px solid #213555;
    transition: all 0.3s ease-in-out;
}

QPushButton:hover {
    background-color: #f5efe7;
    color: #213555;
    border: 2px solid #3e5879;
}

QPushButton:pressed {
    background-color: #213555;
    color: white;
    border: 2px solid #3e5879;
}

