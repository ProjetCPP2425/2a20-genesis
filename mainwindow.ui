      <string notr="true">QComboBox {
    background: #ffffff; /* White background for a clean look */
    border: 2px solid #213555; /* Dark blue border */
    border-radius: 12px; /* Rounded corners */
    padding: 8px; /* Padding inside the combobox */
    font-size: 13px; /* Reduced font size */
    color: #333333; /* Dark text color for readability */
    selection-background-color: #1e90ff; /* Blue selection highlight */
    selection-color: #ffffff; /* White text on selection */
    font-weight: bold;
    letter-spacing: 1px; /* Spacing between letters */
    transition: all 0.3s ease; /* Smooth transition for focus effect */
}

/* Focus state */
QComboBox:focus {
    border: 2px solid #00bfff; /* Light blue border on focus */
    background: #f5f5f5; /* Light gray background on focus */
    box-shadow: 0 4px 10px rgba(30, 144, 255, 0.4); /* Light blue shadow on focus */
}

/* Drop-down button */
QComboBox::drop-down {
    background: #ffffff; /* White background for the drop-down */
    border: none; /* No border */
}

/* Arrow (reset to default arrow) */
QComboBox::down-arrow {
    image: none; /* Ensure no custom image is set */
    width: 10px; /* Size of the default arrow */
    height: 10px; /* Size of the default arrow */
    border: none; /* Remove any border */
}

/* Item view (the list of options) */
QComboBox QAbstractItemView {
    background: #ffffff; /* White background for the drop-down list */
    border: 1px solid #213555; /* Dark border around the list */
    selection-background-color: #1e90ff; /* Blue background for selected item */
    selection-color: #ffffff; /* White text for selected item */
    font-size: 13px; /* Font size of the list items */
}

/* Items inside the drop-down */
QComboBox QAbstractItemView::item {
    padding: 10px; /* Padding for each item */
    font-size: 13px; /* Font size of the items */
}
