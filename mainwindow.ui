   <string notr="true">/* 🌟 Global Styles */
* {
    font-family: Arial, sans-serif;
    color: #213555; /* Dark blue for text */
}

/* 🌟 Background - Soft Beige Gradient */
QWidget {
    background: qlineargradient(
        spread: pad,
        x1: 0, y1: 0,
        x2: 1, y2: 1,
        stop: 0 #f5efe7,
        stop: 1 #e6e0d7
    );
}

/* 🌟 Group Box - Forms &amp; Sections */
QGroupBox {
    background-color: #f5efe7;
    border: 2px solid #3e5879;
    border-radius: 10px;
    margin-top: 20px;
    padding: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    background-color: #3e5879;
    color: white;
    padding: 6px 12px;
    border-radius: 5px;
    font-size: 14px;
    font-weight: bold;
}

/* 🌟 Input Fields */
QLineEdit,
QTextEdit,
QComboBox,
QDateEdit,
QTimeEdit {
    border: 2px solid #3e5879;
    border-radius: 8px;
    padding: 6px;
    font-size: 14px;
    background-color: #ffffff;
}

QLineEdit:focus,
QTextEdit:focus,
QComboBox:focus,
QDateEdit:focus,
QTimeEdit:focus {
    border: 2px solid #213555;
}

/* 🌟 Buttons */
QPushButton {
    background-color: #3e5879;
    color: #ffffff;
    border-radius: 12px;
    padding: 10px 20px;
    font-size: 14px;
    font-weight: bold;
    border: 2px solid #213555;
    transition: all 0.3s ease-in-out;
}

QPushButton:hover {
    background-color: #f5efe7;
    color: #213555;
    border: 2px solid #3e5879;
}

QPushButton:pressed {
    background-color: #213555;
    color: white;
    border: 2px solid #3e5879;
}

/* 🌟 Action Buttons */
QPushButton#edit {
    background-color: #3e5879;
    color: white;
    border-radius: 6px;
    padding: 6px;
}

QPushButton#delete {
    background-color: #d9534f;
    color: white;
    border-radius: 6px;
    padding: 6px;
}

QPushButton#delete:hover {
    background-color: #c9302c;
}

/* 🌟 Tables */
QTableWidget {
    border: 2px solid #3e5879;
    background-color: #f5efe7;
    gridline-color: #3e5879;
}

QHeaderView::section {
    background-color: #213555;
    color: white;
    padding: 7px;
    font-weight: bold;
    border: none;
    border-radius: 5px;
}

QTableWidget::item:hover {
    background-color: #d6cec6;
}

/* 🌟 Sidebar */
QFrame#sidebar {
    background-color: #213555;
    color: white;
    border-radius: 10px;
}

QListWidget#menu {
    background: transparent;
    color: #f5efe7;
    font-size: 14px;
    border: none;
}

QListWidget#menu::item:hover {
    background-color: #3e5879;
    color: white;
    border-radius: 5px;
    padding: 5px;
}

/* 🌟 Search Bar */
QLineEdit#searchBar {
    border: 2px solid #3e5879;
    border-radius: 8px;
    padding: 6px;
    font-size: 14px;
    background-color: #ffffff;
}

QLineEdit#searchBar:focus {
    border: 2px solid #213555;
